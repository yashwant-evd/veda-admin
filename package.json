    "build-dev-start": "env-cmd -f .env.development serve -s build",
    "start-prod": "env-cmd -f .env.production react-scripts --max_old_space_size=3072 start",
    "build-prod": "env-cmd -f .env.production react-scripts --max_old_space_size=3072 build",
    "build-prod-start": "env-cmd -f .env.production serve -s build"
  },
